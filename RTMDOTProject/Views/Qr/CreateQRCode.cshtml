
@{
    ViewData["Title"] = "View QR";
    Layout = "~/Views/Shared/sidemaster.cshtml";
}


<div class="row">
    <div class="col-xl-12 ui-sortable">
        <div class="panel panel-inverse">
            <div class="panel-heading">
                <h4 class="panel-title">QR Details &nbsp;&nbsp;</h4>
                <div class="panel-heading-btn">
                </div>
            </div>
            <div class="panel-body">
                <div class="row" >
                    <div style="margin:auto">
                        <img style="height:450px;width:450px; margin:auto" src="@ViewBag.QrCodeUri" class="img-thumbnail" />
                    </div>
                </div>
                <div class="row">
                    <a href="@ViewBag.QrCodeUri" download="@ViewBag.fname" >DOWNLOAD</a>
                </div>
            </div>
        </div>
    </div>

</div>
